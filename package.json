    "@angular/platform-browser-dynamic": "v16-lts",
    "@angular/router": "v16-lts",
    "rxjs": "7.8.1",
    "tslib": "2.6.2",
    "zone.js": "0.13.3",
    "@angular/material": "v16-lts"
  }
