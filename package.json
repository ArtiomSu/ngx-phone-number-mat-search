{
  "name": "my-workspace",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "purgemaps": "rm -rf dist/ngx-phone-number-mat-search/bundles/artiomsu-ngx-phone-number-mat-search.umd.js.map && rm dist/ngx-phone-number-mat-search/fesm2022/artiomsu-ngx-phone-number-mat-search.mjs.map",
    "build-lib": "ng build ngx-phone-number-mat-search && npm run purgemaps",
    "npm-pack": "cd dist/ngx-phone-number-mat-search && npm pack",
    "package": "npm run build-lib && npm run npm-pack",
    "publish": "npm publish $(command ls dist/ngx-phone-number-mat-search/*.tgz)",
    "packagepublish": "npm run package && npm run publish"

  },
  "private": true,
  "dependencies": {
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "v16-lts",
    "@angular/cli": "v16-lts",
    "@angular/compiler-cli": "v16-lts",
    "@types/jasmine": "5.1.4",
    "@types/node": "20.10.4",
    "jasmine-core": "5.1.1",
    "karma": "6.4.2",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "ng-packagr": "16.2.3",
    "typescript": "4.9.3",
    "@angular/animations": "v16-lts",
    "@angular/common": "v16-lts",
    "@angular/compiler": "v16-lts",
    "@angular/core": "v16-lts",
