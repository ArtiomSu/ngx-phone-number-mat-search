{
  "name": "my-workspace",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "purgemaps": "rm dist/ngx-phone-number-mat-search/bundles/artiomsu-ngx-phone-number-mat-search.umd.js.map && rm dist/ngx-phone-number-mat-search/fesm2015/artiomsu-ngx-phone-number-mat-search.js.map",
    "build-lib": "ng build ngx-phone-number-mat-search && npm run purgemaps",
    "npm-pack": "cd dist/ngx-phone-number-mat-search && npm pack",
    "package": "npm run build-lib && npm run npm-pack",
    "publish": "npm publish $(command ls dist/ngx-phone-number-mat-search/*.tgz)",
    "packagepublish": "npm run package && npm run publish"

  },
  "private": true,
  "dependencies": {
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.1.2",
    "@angular/cli": "~12.1.2",
    "@angular/compiler-cli": "~12.1.2",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^12.1.2",
    "typescript": "~4.3.2",
    "@angular/animations": "~12.1.2",
    "@angular/common": "~12.1.2",
    "@angular/compiler": "~12.1.2",
    "@angular/core": "~12.1.2",
    "@angular/forms": "~12.1.2",
    "@angular/platform-browser": "~12.1.2",
    "@angular/platform-browser-dynamic": "~12.1.2",
    "@angular/router": "~12.1.2",
    "rxjs": "~6.6.0",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4",
    "@angular/material": "^12.2.0"
  }
}
